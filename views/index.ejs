<!DOCTYPE html>
<!--[if IE 8]>         <html class="no-js lt-ie9" lang="en" > <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en" > <!--<![endif]-->
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>sha256</title>
    <link rel="stylesheet" href="/foudnation/css/normalize.css" />
    <link rel="stylesheet" href="/foundation/css/foundation.css" />
    <script src="/foundation/js/vendor/custom.modernizr.js"></script>
  </head>
  <body>
    <div class="row">
      <div class="large-12 columns">
        <h1>sha256 <small>A free utility for checking hashes</small></h1>
        <hr />
      </div>
    </div>

    <div class="row">
      <div class="larger-12 columns">
        <h6>Input:</h6>
        <div>
          <%= value %>
        </div>
        <hr />
      </div>
    </div>
    <div class="row">
      <div class="larger-12 columns">
        <h6>Output:</h6>
        <div>
          <%= hash %>
        </div>
      </div>
    </div>

    <footer class="row">
      <div class="large-12 columns">
        <hr />
        <div class="row">
          <div class="large-6 columns">
            <p><a href="https://github.com/justinpincar/sha256" target="_blank">https://github.com/justinpincar/sha256</a></p>
          </div>
        </div>
      </div>
    </footer>
  </body>
</html>

